    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "edge.js": "^6.2.1",
    "ethers": "^6.15.0",
    "firebase-admin": "^13.4.0",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.1.0",
