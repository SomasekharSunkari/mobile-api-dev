    "libphonenumber-js": "^1.12.7",
    "lodash": "^4.17.21",
    "luxon": "^3.6.1",
    "mathjs": "^14.9.1",
    "ms": "^2.1.3",
    "multer": "^2.0.1",
    "nest-winston": "^1.10.2",
    "node-device-detector": "^2.2.0",
    "node-forge": "^1.3.3",
    "objection": "^3.1.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
