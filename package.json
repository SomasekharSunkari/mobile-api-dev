{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "sign:zh:o": "ts-node test/utils/generate-zerohash-signature.ts o",
    "sign:zh:p": "ts-node test/utils/generate-zerohash-signature.ts p",
    "sign:zh:po": "ts-node test/utils/generate-zerohash-signature.ts po",
    "generate:git-commit": "node scripts/write-git-commit.js",
    "generate:test-certs": "ts-node scripts/generate-test-certs.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:silent": "jest --silent --forceExit",
    "test:completely-silent": "jest --config jest.silent.config.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --forceExit",
    "test:ci": "jest --coverage --forceExit --maxWorkers=4 --detectOpenHandles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:debug": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "mg:up": "knex migrate:latest",
    "mg:down": "knex migrate:down",
    "mg:make": "knex migrate:make",
    "mg:rollback": "knex migrate:rollback --all",
    "db:seed": "knex seed:run",
    "prepare": "husky",
    "ts": "tsc-node",
    "mock": "ts-node src/mocks/commands/index.ts"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
